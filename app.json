{
	"expo": {
		"name": "clovebook-mobile",
		"slug": "clovebook-mobile",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/clove-icon.png",
		"splash": {
			"image": "./assets/clove-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#e1eaeb"
		},
		"updates": {
